{
  "name": "@treblle/utils",
  "version": "0.1.3",
  "description": "Common utilities needed for Treblle JavaScript SDKs",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli build",
    "build": "npm run compile",
    "lint": "npx prettier --check .",
    "lint:fix": "npx prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [
    "Treblle",
    "Treblle SDK",
    "utils"
  ],
  "author": "Dominus_Kelvin, Treblle",
  "license": "MIT",
  "devDependencies": {
    "del-cli": "^5.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
